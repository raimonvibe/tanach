{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/reader",
      "destination": "/reader.html"
    },
    {
      "source": "/calendar",
      "destination": "/calendar.html"
    }
  ]
}
